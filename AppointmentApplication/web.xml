<?xml version="1.0" encoding="UTF-8"?>
<web-app>

	<servlet>
		<servlet-name>first</servlet-name>
		<servlet-class>com.mondee.DoctorRegister</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>first</servlet-name>
		<url-pattern>/doc_register</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>second</servlet-name>
		<servlet-class>com.mondee.Doctor_Select</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>second</servlet-name>
		<url-pattern>/select</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>third</servlet-name>
		<servlet-class>com.mondee.DoctorLogin</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>third</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	

</web-app>